import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        String s = scan.nextLine();
        String[] str = s.split("[^a-zA-Z]");
        int numTokens=0;
        
        for (String strX : str) {
            if(strX.length()>0) {
                numTokens++;
            }
        }
        System.out.println(numTokens);
        for (String strX : str) {
            if(strX.length()>0) {
                System.out.println(strX);
            }
        }         
        scan.close();
    }
}
